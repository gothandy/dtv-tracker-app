{
  "name": "volunteer-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "node app.js",
    "dev": "nodemon app.js",
    "dev:members": "dotenv -e .env.members -- nodemon app.js",
    "dev:tracker": "dotenv -e .env.tracker -- nodemon app.js",
    "test": "echo \"No tests configured\""
